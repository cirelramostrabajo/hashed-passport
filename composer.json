{
    "name": "cirelramostrabajo/hashed-passport",
    "description": "Transforms Laravel Passport's default incrementing integer client_id into an industry standard unique hashed string. Optionally, you can use encrypted client secrets for improved security. The package is non-intrusive. See the readme for details.",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cirelramostrabajo/hashed-passport/issues",
        "source": "https://github.com/cirelramostrabajo/hashed-passport"
    },
    "authors": [
        {
            "name": "Cirel Ramos",
            "email": "cirelramostrabajo@gmail.com"
        },
        {
            "name": "Cirel Ramos",
            "email": "cirelramostrabajo@gmail.com"
        }
    ],
    "require": {
        "laravel/passport": "^9.4|^10.1",
	"vinkla/hashids": "^8.1",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "mockery/mockery": ">=1.3",
        "orchestra/testbench": "^5.10",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Cirelramostrabajo\\HashedPassport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cirelramostrabajo\\HashedPassport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "Cirelramostrabajo\\HashedPassport\\HashedPassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
